<%- include('../partials/head') %>
<body class="h-100">
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-2"><%- include('../partials/sidebar') %></div>
            <div class="col-sm-10">
                <h1><%= title %></h1>
                <ul>
                    <% category_list.forEach(function(category) { %>
                    <li>
                        <span
                            ><a href="<%= category.url %>"
                                ><%= category.name %></a
                            >
                        </span>
                        <!-- <p><%= category.description %></p> -->
                    </li>
                    <% }) %>
                </ul>
            </div>
        </div>
    </div>
</body>
